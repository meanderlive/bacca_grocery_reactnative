{
  "name": "Justskipline",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-simulator": "react-native run-ios --simulator='iPhone 15'",
    "ios-device": "react-native run-ios --device",
    "lint": "eslint .",
    "start": "react-native start",
    "clean": "cd android && ./gradlew clean  && cd ..",
    "clean-ios": "cd ios && rm -rf build && rm -rf Pods && rm -rf Podfile.lock && cd ..",
    "pod-install": "cd ios && pod install && cd ..",
    "build": "cd android && ./gradlew bundleRelease && cd ..",
    "build-ios": "cd ios && xcodebuild -workspace Justskipline.xcworkspace -scheme Justskipline -configuration Release -destination generic/platform=iOS -archivePath Justskipline.xcarchive archive && cd ..",
    "ls-key": "cd android && ./gradlew signingReport && cd ..",
    "vr": "npx react-native run-android --variant=release",
    "test": "jest",
    "dev": "react-native start --experimental-debugger",
    "startc": "react-native start --reset-cache",
    "apk": "yarn && cd android && ./gradlew assembleRelease && cd ..",
    "apkk": "yarn android --mode release",
    "push": "git add . && git commit -m 'update' && git push"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/datetimepicker": "^8.4.5",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-firebase/app": "^23.3.1",
    "@react-native-firebase/messaging": "^23.3.1",
    "@react-native-picker/picker": "^2.11.2",
    "@react-native/new-app-screen": "0.81.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@reduxjs/toolkit": "^2.9.0",
    "@shopify/flash-list": "^2.0.3",
    "@stripe/stripe-react-native": "^0.53.0",
    "@types/react-native-snap-carousel": "^3.8.12",
    "@types/react-native-vector-icons": "^6.4.18",
    "formik": "^2.4.6",
    "lottie-ios": "^4.5.1",
    "lottie-react-native": "^7.3.4",
    "react": "19.1.0",
    "react-native": "0.80.1",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-animatable": "^1.4.0",
    "react-native-config": "^1.5.9",
    "react-native-countdown-circle-timer": "^3.2.1",
    "react-native-date-picker": "^5.0.13",
    "react-native-dotenv": "^3.4.11",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-crop-picker": "^0.51.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-immersive-bars": "^1.0.4",
    "react-native-keyboard-controller": "^1.18.6",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-otp-inputs": "^7.4.0",
    "react-native-permissions": "^5.4.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-ratings": "^8.1.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-svg": "^15.13.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-webview": "^13.16.0",
